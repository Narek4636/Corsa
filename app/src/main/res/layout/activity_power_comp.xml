<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:background="@drawable/background"
    android:orientation="vertical"
    tools:context="com.samsung.corsa.modes.PowerCompActivity">

    <FrameLayout
        android:id="@+id/status_bar_power_comp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <include
        layout="@layout/menu_button_layout"
        />

    <TextView
        android:id="@+id/title_power_comp"
        style="@style/Title"
        android:fontFamily="@font/exo_2_extrabold"
        android:text="WHICH ONE HAS MORE POWER?"
        app:layout_constraintEnd_toEndOf="@id/guideline9_power_comp"
        app:layout_constraintStart_toStartOf="@id/guideline8_power_comp"
        app:layout_constraintBottom_toTopOf="@+id/guideline11_power_comp"
        app:layout_constraintTop_toTopOf="@+id/guideline10_power_comp" />

    <TextView
        android:id="@+id/name1_power_comp"
        style="@style/Comp_carname"
        android:text="FERRARI 599 GTB FIORANO"
        app:layout_constraintBottom_toBottomOf="@+id/guideline12_power_comp"
        app:layout_constraintEnd_toEndOf="@+id/guideline2_power_comp"
        app:layout_constraintStart_toStartOf="@+id/guideline1_power_comp"
        app:layout_constraintTop_toTopOf="@+id/guideline13_power_comp" />

    <TextView
        android:id="@+id/year1_power_comp"
        style="@style/Comp_caryear"
        android:text="2006"
        app:layout_constraintBottom_toBottomOf="@+id/image1_power_comp"
        app:layout_constraintEnd_toEndOf="@+id/image1_power_comp"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="@+id/image1_power_comp"
        app:layout_constraintTop_toBottomOf="@+id/name1_power_comp"
        app:layout_constraintVertical_bias="0.02" />

    <TextView
        android:id="@+id/power1_power_comp"
        style="@style/Comp_answer"
        android:text="100"
        app:layout_constraintBottom_toBottomOf="@+id/image1_power_comp"
        app:layout_constraintEnd_toEndOf="@+id/image1_power_comp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="@+id/image1_power_comp"
        app:layout_constraintTop_toTopOf="@+id/image1_power_comp" />

    <TextView
        android:id="@+id/hp1_power_comp"
        style="@style/Comp_answer"
        android:text="HP"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/image1_power_comp"
        app:layout_constraintEnd_toEndOf="@+id/image1_power_comp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="@+id/image1_power_comp"
        app:layout_constraintTop_toBottomOf="@+id/power1_power_comp"
        app:layout_constraintVertical_bias="0.0" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/image1_power_comp"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        android:src="@drawable/test_pic"
        app:layout_constraintDimensionRatio="4:3"
        app:layout_constraintEnd_toEndOf="@id/guideline2_power_comp"
        app:layout_constraintStart_toStartOf="@id/guideline1_power_comp"
        app:layout_constraintTop_toTopOf="@+id/guideline3_power_comp"
        app:shapeAppearanceOverlay="@style/Rounded" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/image1_black_power_comp"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:alpha="0.25"
        android:scaleType="centerCrop"
        android:src="@drawable/black_background"
        app:layout_constraintBottom_toBottomOf="@id/image1_power_comp"
        app:layout_constraintEnd_toEndOf="@id/image1_power_comp"
        app:layout_constraintStart_toStartOf="@id/image1_power_comp"
        app:layout_constraintTop_toTopOf="@+id/image1_power_comp"
        app:shapeAppearanceOverlay="@style/Rounded" />

    <TextView
        android:id="@+id/name2_power_comp"
        style="@style/Comp_carname"
        android:text="RUF RT12"
        app:layout_constraintBottom_toBottomOf="@+id/guideline15_power_comp"
        app:layout_constraintEnd_toEndOf="@+id/guideline2_power_comp"
        app:layout_constraintStart_toStartOf="@+id/guideline1_power_comp"
        app:layout_constraintTop_toTopOf="@+id/guideline14_power_comp"
        app:layout_constraintVertical_bias="0.07" />


    <TextView
        android:id="@+id/year2_power_comp"
        style="@style/Comp_caryear"
        android:text="2005"
        app:layout_constraintBottom_toBottomOf="@+id/image2_power_comp"
        app:layout_constraintEnd_toEndOf="@+id/image2_power_comp"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="@+id/image2_power_comp"
        app:layout_constraintTop_toBottomOf="@+id/name2_power_comp"
        app:layout_constraintVertical_bias="0.02" />

    <TextView
        android:id="@+id/power2_power_comp"
        style="@style/Comp_answer"
        android:text="100"
        app:layout_constraintBottom_toBottomOf="@+id/image2_power_comp"
        app:layout_constraintEnd_toEndOf="@+id/image2_power_comp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="@+id/image2_power_comp"
        app:layout_constraintTop_toTopOf="@+id/image2_power_comp" />

    <TextView
        android:id="@+id/hp2_power_comp"
        style="@style/Comp_answer"
        android:text="HP"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/image2_power_comp"
        app:layout_constraintEnd_toEndOf="@+id/image2_power_comp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="@+id/image2_power_comp"
        app:layout_constraintTop_toBottomOf="@+id/power2_power_comp"
        app:layout_constraintVertical_bias="0.0" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/image2_power_comp"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        android:src="@drawable/test_pic"
        app:layout_constraintDimensionRatio="4:3"
        app:layout_constraintEnd_toEndOf="@id/guideline2_power_comp"
        app:layout_constraintStart_toStartOf="@id/guideline1_power_comp"
        app:layout_constraintTop_toTopOf="@id/guideline4_power_comp"
        app:layout_constraintVertical_bias="0.25"
        app:shapeAppearanceOverlay="@style/Rounded" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/image2_black_power_comp"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:alpha="0.25"
        android:scaleType="centerCrop"
        android:src="@drawable/black_background"
        app:layout_constraintBottom_toBottomOf="@id/image2_power_comp"
        app:layout_constraintEnd_toEndOf="@id/image2_power_comp"
        app:layout_constraintStart_toStartOf="@id/image2_power_comp"
        app:layout_constraintTop_toTopOf="@id/image2_power_comp"
        app:layout_constraintVertical_bias="0.25"
        app:shapeAppearanceOverlay="@style/Rounded" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline1_power_comp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.03" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2_power_comp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.97" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3_power_comp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.23" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4_power_comp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.63" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5_power_comp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.99" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline6_power_comp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.007" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline7_power_comp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.03" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline8_power_comp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.14" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline9_power_comp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.86" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline10_power_comp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline11_power_comp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.2" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline12_power_comp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.275" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline13_power_comp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.245" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline14_power_comp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.6415" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline15_power_comp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.6715" />


</androidx.constraintlayout.widget.ConstraintLayout>