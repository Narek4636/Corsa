<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:orientation="vertical"
    android:elevation="3dp"
    tools:context=".StatusBarFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/status_bar_fragment"
        android:layout_gravity="center"
        android:orientation="vertical"
        >

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/background1_status"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/button_oval_shape"
        app:layout_constraintBottom_toBottomOf="@id/guideline13_status"
        app:layout_constraintEnd_toEndOf="@id/guideline3_status"
        app:layout_constraintStart_toStartOf="@id/guideline2_status"
        app:layout_constraintTop_toTopOf="@+id/guideline1_status" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/background2_status"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/button_oval_shape"
        app:layout_constraintBottom_toBottomOf="@+id/guideline11_status"
        app:layout_constraintEnd_toEndOf="@id/guideline3_status"
        app:layout_constraintStart_toStartOf="@id/guideline2_status"
        app:layout_constraintTop_toTopOf="@id/guideline4_status" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/lvlBG_text_status"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/level_shape"
        android:elevation="2dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="@+id/guideline13_status"
        app:layout_constraintEnd_toEndOf="@id/guideline3_status"
        app:layout_constraintStart_toStartOf="@+id/guideline12_status"
        app:layout_constraintTop_toTopOf="@+id/background1_status" />

    <TextView
        android:id="@+id/lvl_text_status"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:autoSizeTextType="uniform"
        android:elevation="2dp"
        android:fontFamily="@font/exo_2_extrabold"
        android:gravity="center"
        android:text="LEVEL"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/guideline15_status"
        app:layout_constraintEnd_toEndOf="@id/guideline10_status"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/guideline9_status"
        app:layout_constraintTop_toTopOf="@+id/guideline14_status" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/ratingBG_text_status"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/level_shape"
        android:elevation="2dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="@+id/guideline11_status"
        app:layout_constraintEnd_toEndOf="@id/guideline3_status"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/guideline12_status"
        app:layout_constraintTop_toTopOf="@+id/guideline4_status" />
    />

    <TextView
        android:id="@+id/rating_text_status"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:autoSizeTextType="uniform"
        android:autoSizeMinTextSize="1dp"

        android:elevation="2dp"
        android:fontFamily="@font/exo_2_extrabold"
        android:gravity="center"
        android:text="XP"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/guideline6_status"
        app:layout_constraintEnd_toEndOf="@id/guideline10_status"
        app:layout_constraintStart_toStartOf="@+id/guideline9_status"
        app:layout_constraintTop_toTopOf="@+id/guideline5_status" />

    <TextView
        android:id="@+id/level"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:autoSizeTextType="uniform"
        android:autoSizeMinTextSize="1dp"
        android:elevation="2dp"
        android:fontFamily="@font/exo_2_extrabold"
        android:gravity="center"
        android:text="0"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@id/guideline15_status"
        app:layout_constraintEnd_toEndOf="@+id/guideline8_status"
        app:layout_constraintStart_toStartOf="@+id/guideline7_status"
        app:layout_constraintTop_toTopOf="@id/guideline14_status" />

    <TextView
        android:id="@+id/rating"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:autoSizeTextType="uniform"
        android:autoSizeMinTextSize="1dp"
        android:elevation="2dp"
        android:fontFamily="@font/exo_2_extrabold"
        android:gravity="center"
        android:text="0"
        android:textAlignment="center"
        android:textColor="@color/black"
